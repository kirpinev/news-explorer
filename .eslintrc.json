{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-undef": "off",
    "no-useless-constructor": "off",
    "class-methods-use-this": "off",
    "no-else-return": "off",
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
